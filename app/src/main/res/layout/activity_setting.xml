                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/dp_17"
                    android:gravity="center_vertical"
                    android:text="清除缓存"
                    android:textColor="@color/color_black"
                    android:textSize="@dimen/sp_16" />

